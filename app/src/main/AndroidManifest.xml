<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cloudclient.clients">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cloud_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".Client" />
        <activity android:name=".ClientRecord" />
        <activity android:name=".AddClient" />
        <activity android:name=".ClientUpdate" />
        <activity android:name=".ExpensesRecord" />
        <activity android:name=".Reservation" />
        <activity android:name=".Transaction" />
        <activity android:name=".Setting" />
        <activity android:name=".Password_Change" />
        <activity android:name=".Reservation_Add" />
        <activity android:name=".Reservation_record"></activity>
        <receiver android:name=".Alarm"/>
    </application>

</manifest>